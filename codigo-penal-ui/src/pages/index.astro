<BaseLayout title="Código Penal del Perú">
  <div class="relative grid min-h-screen grid-cols-1 gap-y-6 px-4 sm:px-6 lg:grid-cols-5 lg:gap-x-10 md:mx-8 xl:mx-0">
    <SidebarNav tocGroups={tocGroups} />

    <main class="col-span-full w-full max-w-5xl flex-1 lg:col-span-3 lg:col-start-2 lg:justify-self-center">
